<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.4/Chart.js"></script>
<body>
       <div class="container mt-4">
        <div class="row">
             <h5 class="card-title"></h5>

<canvas id="myChart" ></canvas>
<div style="margin-left: 110px;">
<div class="m-5" id="myPlot" style="width:60%;max-width:700px margin-left:100px"></div>
</div>
</div>

</div>
<script>
var xValues = ['COD','ONLINE'];
var yValues = [45,85];
var barColors = [
  "#b91d47",
  "#00aba9",
  "#2b5797",
  "#e8c3b9",
  "#1e7145"
];

new Chart("myChart", {
  type: "doughnut",
  data: {
    labels: xValues,
    datasets: [{
      backgroundColor: barColors,
      data: yValues
    }]
  },
  options: {
    title: {
      display: true,
      text: "Doughnut chart"
    }
  }
});
</script>


</body>

</div>





