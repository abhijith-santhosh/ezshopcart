
     {{>usernav}}
    <section>
        <div class="container mt-5">
            <div class="row">
                
                {{#each products}}
                <div class="col-md-3">
                    <div class="card" style="width: 15rem; height:25rem">
                        <img class="card-img-top"
                            src="/images/{{this.product.image.[0]}}"
                            alt="Card image cap" style="height:12rem;">
                        <div class="card-body">
                            <h5 class="card-title">{{this.product.name}}</h5>
                             <h5 class="card-title">â‚¹{{this.product.Prize}}/-</h5>
                              <h5 class="card-title " >Quantity-{{this.quantity}}</h5>
                            <p class="card-text">{{this.product.Description}}</p>
                           
                        </div>
                    </div>
                </div>
                 {{/each}}
                 
            </div>
        </div>
        
    </section>

    


     

      

     