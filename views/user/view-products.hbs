 
    
      {{>usernav}}
    <section>
        <div class="container mt-5">
            <div class="row">
                
                {{#each products}}
                <div class="col-md-3">
                    <div class="card" style="width: 15rem; height:25rem">
                        <img class="card-img-top"
                            src="/product-images/{{this._id}}.jpg"
                            alt="Card image cap" style="height:12rem;">
                        <div class="card-body">
                            <h5 class="card-title">{{this.name}}</h5>
                             <h5 class="card-title">{{this.Prize}}</h5>
                            <p class="card-text">{{this.Description}}</p>
                              <button onclick="addToCart('{{this._id}}')"  class="btn btn-success"> Add Cart </button>    
                            
                        </div>
                    </div>
                </div>
                 {{/each}}
                 
            </div>
        </div>
        
    </section>

    


     

     